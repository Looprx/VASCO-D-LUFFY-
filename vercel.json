{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://vascodluffy.vercel.app",
    "NEXT_PUBLIC_APP_NAME": "VASCO D LUFFY Drug Safety Platform"
  },
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install"
}